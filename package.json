{
  "name": "@inkline/inkline",
  "version": "1.8.3",
  "description": "Inkline is a modern UI/UX framework for Vue.js, designed for creating flawless content-rich responsive web applications",
  "author": "Alex Grozav <alex@grozav.com>",
  "scripts": {
    "build": "vue-cli-service build && npm run dist",
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service serve",
    "test": "vue-cli-service test:unit --coverage --no-cache",
    "test:dev": "vue-cli-service test:unit --watch --coverage --no-cache",
    "coveralls": "cat ./tests/coverage/lcov.info | coveralls",
    "dist": "./scripts/dist.sh"
  },
  "dependencies": {
    "popper.js": "^1.14.6",
    "validator": "^10.10.0"
  },
  "peerDependencies": {
    "vue": ">=2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/types": "^7.2.2",
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-plugin-unit-jest": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "@vue/test-utils": "^1.0.0-beta.28",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "coveralls": "^3.0.2",
    "postcss-preset-env": "^5.3.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue": "^2.6.6",
    "vue-template-compiler": "^2.6.6"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "postcss-preset-env": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "bugs": {
    "url": "https://github.com/inkline/inkline/issues"
  },
  "homepage": "http://inkline.io",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inkline/inkline.git"
  },
  "main": "dist/js/inkline.js",
  "files": [
    "dist/",
    "src/",
    "babel.config.js",
    "LICENSE",
    "package.json",
    "README.md",
    "vue.config.js"
  ]
}
